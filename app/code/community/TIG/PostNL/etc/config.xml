    <!-- Custom TiG config values -->
    <tig>
        <!-- Version compatibility per Magento edition -->
        <compatibility>
            <postnl>
                <enterprise>1.11,1.12,1.13</enterprise>
                <community>1.6,1.7,1.8</community>
            </postnl>
        </compatibility>
        
        <!-- Stability of this release -->
        <stability>
            <postnl>beta</postnl>
        </stability>
    
        <!-- A list of possible error codes recognized by this extension. Each error has a default message, a link to 
            the associated knowledge base article and a 'type' (whether it's an error, warning or notice) -->
        <errors>
            <!-- These are errors that are generated by this extension -->
            <POSTNL-0001>
                <message>You have configured the extension for test mode only. You still must enter a live username and password before you may use the extension for real shipments.</message>
                <url>http://servicedesk.totalinternetgroup.nl/entries/31114923</url>
                <type>warning</type>
            </POSTNL-0001>
            <POSTNL-0002>
                <message>You have not yet fully configured the extension. You must complete this process before you can use it.</message>
                <url>http://servicedesk.totalinternetgroup.nl/entries/31565256</url>
                <type>error</type>
            </POSTNL-0002>
            <POSTNL-0003>
                <message>You have not yet configured the extension to use GlobalPack shipments. Please complete this process if you wish to ship parcels to outside the EU.</message>
                <url>http://servicedesk.totalinternetgroup.nl/entries/31566436</url>
                <type>notice</type>
            </POSTNL-0003>
            <POSTNL-0004>
                <message>You have not yet configured PostNL Checkout. Please complete this process if you wish to use PostNL Checkout.</message>
                <url>http://servicedesk.totalinternetgroup.nl/entries/31567046</url>
                <type>notice</type>
